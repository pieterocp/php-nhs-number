{
    "name": "fredbradley/php-nhs-number",
    "description": "Utility class to validate, format and generate NHS numbers.",
    "keywords": [
        "imliam",
        "php-nhs-number"
    ],
    "homepage": "https://github.com/imliam/php-nhs-number",
    "license": "MIT",
    "authors": [
        {
            "name": "Liam Hammett",
            "email": "liam@liamhammett.com",
            "homepage": "https://liamhammett.com",
            "role": "Original Developer"
        },
        {
            "name": "Fred Bradley",
            "email": "code@fredbradley.co.uk",
            "homepage": "https://www.fredbradley.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "^9.3",
        "phpunit/phpunit": "~9.0",
        "squizlabs/php_codesniffer": "^3.10"
    },
    "autoload": {
        "psr-4": {
            "ImLiam\\NhsNumber\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ImLiam\\NhsNumber\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "codesniffer": "vendor/bin/phpcs src/ tests/",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "test:windows": "vendor\\bin\\phpunit",
        "test-coverage:windows": "vendor\\bin\\phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
